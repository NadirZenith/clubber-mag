<?xml version="1.0" encoding="UTF-8"?>
<forms version="1.7.5">
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1" postAuthor="2" postCategory="1" postFormat="0" postStatus="pending">
		<title><![CDATA[Evento]]></title>
		<description><![CDATA[COMPLETA EL FORMULARIO DE TU EVENTO]]></description>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<button type="text">
			<text><![CDATA[Enviar]]></text>
		</button>
		<fields>
			<field id="1" isRequired="1" size="large" type="post_title">
				<description><![CDATA[Escribe el nombre de tu evento]]></description>
				<cssClass><![CDATA[col-2-4 pb5 pt5 mt15 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Titulo del evento]]></label>
				<saveAsCPT><![CDATA[event]]></saveAsCPT>
			</field>
			<field id="13" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_place_name" inputType="text">
				<description><![CDATA[Escribe el nombre del Club, local o espacio]]></description>
				<cssClass><![CDATA[col-2-4 pb5 pt5 mt15 fr bg-50 block-5]]></cssClass>
				<label><![CDATA[Nombre del lugar donde se realizará el evento]]></label>
			</field>
			<field id="2" size="large" type="post_content">
				<cssClass><![CDATA[col-2-4 pb5 pt5 mt15 fl bg-50 block-5 cl]]></cssClass>
				<label><![CDATA[Información y descripción general del evento]]></label>
			</field>
			<field id="14" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_place_address" inputType="text">
				<cssClass><![CDATA[col-2-4 pb5 pt5 mt15 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Dirección del Club, local o espacio]]></label>
			</field>
			<field id="7" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_flyer_back" inputType="fileupload">
				<cssClass><![CDATA[col-1-4 pb5 pt5 mt15 fr bg-50 block-5]]></cssClass>
				<label><![CDATA[Back flyer o anexo]]></label>
				<allowedExtensions><![CDATA[jpg, gif, png]]></allowedExtensions>
			</field>
			<field id="6" isRequired="1" size="medium" type="post_image">
				<cssClass><![CDATA[col-1-4 pb5 pt5 mt15 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Subir flyer del evento]]></label>
				<allowedExtensions><![CDATA[jpg, jpeg, png, gif]]></allowedExtensions>
				<postFeaturedImage><![CDATA[1]]></postFeaturedImage>
			</field>
			<field id="9" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_type" inputType="radio" enableChoiceValue="1">
				<cssClass><![CDATA[col-1-4 pb5 pt5 mt15 fr bg-50 block-5]]></cssClass>
				<label><![CDATA[Tipo de evento]]></label>
				<choices>
					<choice isSelected="1">
						<text><![CDATA[Fiesta / Evento]]></text>
						<value><![CDATA[party]]></value>
					</choice>
					<choice>
						<text><![CDATA[Festival]]></text>
						<value><![CDATA[festival]]></value>
					</choice>
				</choices>
			</field>
			<field id="12" isRequired="1" size="medium" type="select">
				<cssClass><![CDATA[col-1-4 pb5 pt5 mt15 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Ciudad]]></label>
				<choices>
					<choice>
						<text><![CDATA[-- Ciudad --]]></text>
					</choice>
					<choice>
						<text><![CDATA[Barcelona]]></text>
					</choice>
					<choice>
						<text><![CDATA[Madrid]]></text>
					</choice>
				</choices>
				<populateTaxonomy><![CDATA[city]]></populateTaxonomy>
			</field>
			<field id="15" size="medium" type="section" displayOnly="1">
				<cssClass><![CDATA[cb]]></cssClass>
			</field>
			<field id="3" isRequired="1" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_begin_date" inputType="text" dateFormat="dmy">
				<cssClass><![CDATA[col-1-4 pb5 pt5 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Fecha inicio]]></label>
			</field>
			<field id="19" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_end_date" inputType="text" dateFormat="dmy">
				<cssClass><![CDATA[col-1-4 pb5 pt5 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Fecha término]]></label>
			</field>
			<field id="16" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_promoter" inputType="text">
				<cssClass><![CDATA[col-1-4 pb5 pt5 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Promotor]]></label>
			</field>
			<field id="17" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_email_confirmation" inputType="email">
				<cssClass><![CDATA[col-1-4 pb5 pt5 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[E-mail de confirmación]]></label>
			</field>
			<field id="8" isRequired="1" size="small" type="post_custom_field" postCustomFieldName="wpcf-event_price" inputType="number" numberFormat="decimal_comma">
				<defaultValue><![CDATA[0]]></defaultValue>
				<description><![CDATA[0 = Gratis]]></description>
				<cssClass><![CDATA[col-1-4 pb5 pt5 fl bg-50 block-5 cl]]></cssClass>
				<label><![CDATA[Precio €]]></label>
			</field>
			<field id="20" size="large" type="post_custom_field" postCustomFieldName="wpcf-event_price_conditions" inputType="text">
				<description><![CDATA[(Ej: Descuentos, lista hasta 02:00 AM, etc)]]></description>
				<cssClass><![CDATA[col-1-4 pb5 pt5 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Condiciones]]></label>
			</field>
			<field id="18" size="medium" type="post_custom_field" postCustomFieldName="wpcf-event_price_conditions" inputType="text">
				<description><![CDATA[Necesitarás una cuenta <a href="#">RR.PP.</a> service]]></description>
				<cssClass><![CDATA[col-2-4 pb5 pt5 fl bg-50 block-5]]></cssClass>
				<label><![CDATA[Lista gratis o de descuento]]></label>
			</field>
		</fields>
		<confirmations>
			<confirmation id="532244825664a" isDefault="1" type="message">
				<name><![CDATA[Default Confirmation]]></name>
				<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
			</confirmation>
		</confirmations>
		<notifications>
			<notification id="532244824e365">
				<to><![CDATA[{admin_email}]]></to>
				<name><![CDATA[Notificación del Administrador]]></name>
				<event><![CDATA[form_submission]]></event>
				<toType><![CDATA[email]]></toType>
				<subject><![CDATA[New submission from {form_title}]]></subject>
				<message><![CDATA[{all_fields}]]></message>
			</notification>
		</notifications>
	</form>
	<form labelPlacement="top_label" useCurrentUserAsAuthor="1">
		<title><![CDATA[Contacto]]></title>
		<description><![CDATA[Formulario de contacto]]></description>
		<descriptionPlacement><![CDATA[below]]></descriptionPlacement>
		<button type="text">
			<text><![CDATA[Enviar]]></text>
		</button>
		<fields>
			<field id="1" isRequired="1" size="medium" type="text">
				<cssClass><![CDATA[col-2-4 pb5 pt5 mt15 bg-50 block-5]]></cssClass>
				<label><![CDATA[Nombre]]></label>
			</field>
			<field id="4" isRequired="1" size="medium" type="email">
				<cssClass><![CDATA[col-2-4 pb5 pt5 mt15 bg-50 block-5]]></cssClass>
				<label><![CDATA[Email]]></label>
			</field>
			<field id="3" isRequired="1" size="medium" type="textarea">
				<cssClass><![CDATA[col-2-4 pb5 pt5 mt15 bg-50 block-5]]></cssClass>
				<label><![CDATA[Contacto]]></label>
			</field>
		</fields>
		<confirmations>
			<confirmation id="5335badd63cdb" isDefault="1" type="message">
				<name><![CDATA[Default Confirmation]]></name>
				<message><![CDATA[Thanks for contacting us! We will get in touch with you shortly.]]></message>
			</confirmation>
		</confirmations>
		<notifications>
			<notification id="5335badd58ee6">
				<to><![CDATA[{admin_email}]]></to>
				<name><![CDATA[Notificación del Administrador]]></name>
				<event><![CDATA[form_submission]]></event>
				<toType><![CDATA[email]]></toType>
				<subject><![CDATA[New submission from {form_title}]]></subject>
				<message><![CDATA[{all_fields}]]></message>
			</notification>
		</notifications>
	</form>
</forms>